{
  "name": "proto-ikon-loader-wc",
  "version": "0.0.99",
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/proto-ikon-loader-wc/proto-ikon-loader-wc.esm.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "start": "stencil build --dev --watch --serve",
    "test": "echo --skip",
    "generate": "stencil generate",
    "prebuild": "yarn format",
    "predeploy": "yarn build",
    "deploy": "yarn publish --patch",
    "spell": "cspell 'src/**/*.{ts,tsx,html,css,md}' ",
    "cc": "prettier --check src cSpell.json",
    "format": "prettier --write src cSpell.json",
    "lint": "eslint src/**/*.{ts,tsx}"
  },
  "dependencies": {
    "@stencil/core": "2.16.1",
    "proto-ikons-wc": "0.0.59"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "5.27.1",
    "@typescript-eslint/parser": "5.27.1",
    "cspell": "6.1.2",
    "eslint": "8.17.0",
    "prettier": "2.6.2",
    "typescript": "4.7.3"
  },
  "license": "MIT"
}
