{
  "name": "proto-ikon-loader-wc",
  "version": "0.0.340",
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/proto-ikon-loader-wc/proto-ikon-loader-wc.esm.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "start": "stencil build --dev --watch --serve",
    "test": "echo --skip",
    "generate": "stencil generate",
    "prebuild": "yarn format",
    "predeploy": "yarn build",
    "deploy": "yarn publish --patch",
    "spell": "cspell 'src/**/*.{ts,tsx,html,css,md}' ",
    "cc": "prettier --check src cSpell.json",
    "format": "prettier --write src cSpell.json",
    "lint": "eslint src/**/*.{ts,tsx}"
  },
  "dependencies": {
    "@stencil/core": "4.12.2",
    "proto-ikons-wc": "0.0.131"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "7.0.1",
    "@typescript-eslint/parser": "7.0.1",
    "cspell": "8.3.2",
    "eslint": "8.56.0",
    "prettier": "3.2.5",
    "typescript": "5.3.3"
  },
  "license": "MIT"
}
